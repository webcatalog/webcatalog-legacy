{
  "name": "Juli",
  "version": "2.0.0",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./dist.js ./public ./src --ext js",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron . --id=test --url=https://messenger.com --name=Messenger\"",
    "electron-dev:no-argv": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "yarn build && node dist.js",
    "test": "mocha ./tests"
  },
