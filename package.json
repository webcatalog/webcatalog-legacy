{
  "name": "WebCatalog",
  "version": "3.4.4",
  "description": "WebCatalog, run your favorite web apps natively",
  "main": "main.js",
  "scripts": {
    "start": "yarn run start.store",
    "start.store": "electron ./app --development=true",
    "start.app": "electron ./app --development=true --id=test --url=https://messenger.com --name=Shaka",
    "start.store.production": "electron ./app",
    "dev": "cross-env NODE_ENV=development webpack -d --watch",
    "compile": "cross-env NODE_ENV=production webpack -p",
    "package": "node ./packager.js",
    "lint": "eslint ./ui --ext .js --cache",
    "dist": "yarn compile && yarn package",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/webcatalog/webcatalog",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com",
    "url": "https://quanglam.me"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "electron": "^1.6.2",
    "electron-builder": "^16.2.0",
    "electron-builder-squirrel-windows": "15.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "spectron": "^3.6.0",
    "webpack": "^2.3.1",
    "webpack-merge": "^4.0.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.13.0",
    "algoliasearch": "^3.22.1",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "lodash.camelcase": "^4.3.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-electron-web-view": "^2.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-batched-actions": "^0.1.5",
    "redux-thunk": "^2.1.0"
  }
}
